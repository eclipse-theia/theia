{
  "name": "@theia/electron",
  "version": "1.58.0",
  "description": "Theia - Electron utility package",
  "dependencies": {
    "electron-store": "^8.0.0",
    "fix-path": "^4.0.0",
    "native-keymap": "^2.2.1"
  },
  "devDependencies": {
    "@theia/ext-scripts": "1.58.0",
    "@theia/re-exports": "1.58.0"
  },
  "peerDependencies": {
    "electron": "30.1.2"
  },
  "theiaReExports": {
    "shared": {
      "export *": [
        "native-keymap"
      ],
      "export =": [
        "electron as Electron",
        "electron-store as ElectronStore",
        "fix-path as fixPath"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [ ],
  "keywords": [
    "theia-extension"
  ],
  "license": "EPL-2.0 OR GPL-2.0-only WITH Classpath-exception-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-theia/theia.git"
  },
  "bugs": {
    "url": "https://github.com/eclipse-theia/theia/issues"
  },
  "homepage": "https://github.com/eclipse-theia/theia",
  "files": [
    "index.d.ts",
    "index.js",
    "shared"
  ],
  "scripts": {
    "afterInstall": "npm run generate-theia-re-exports",
    "generate-theia-re-exports": "theia-re-exports generate && theia-re-exports template README_TEMPLATE.md > README.md"
  },
  "nyc": {
    "extends": "../../configs/nyc.json"
  }
}
