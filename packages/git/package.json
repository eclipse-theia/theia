{
  "name": "@theia/git",
  "version": "1.35.0",
  "description": "Theia - Git Integration",
  "dependencies": {
    "@theia/core": "1.35.0",
    "@theia/editor": "1.35.0",
    "@theia/filesystem": "1.35.0",
    "@theia/monaco-editor-core": "1.72.3",
    "@theia/navigator": "1.35.0",
    "@theia/scm": "1.35.0",
    "@theia/scm-extra": "1.35.0",
    "@theia/workspace": "1.35.0",
    "@types/diff": "^3.2.2",
    "@types/p-queue": "^2.3.1",
    "diff": "^3.4.0",
    "dugite-extra": "0.1.17",
    "find-git-exec": "^0.0.4",
    "find-git-repositories": "^0.1.1",
    "luxon": "^2.4.0",
    "octicons": "^7.1.0",
    "p-queue": "^2.4.2",
    "ts-md5": "^1.2.2"
