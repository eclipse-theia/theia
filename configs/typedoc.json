{
  "emit": false,
  "exclude": [
    "../+(dev-packages|examples|typings|scripts)/**",
    "../packages/*/lib/**",
    "**/node_modules/**",
    "**/*spec.ts",
    "../plugins/*"
  ],
  "excludeExternals": true,
  "external-modulemap": ".*\/packages\/([\\w\\-_]+)\/",
  "hideGenerator": true,
  "name": "Theia TypeDoc",
  "out": "../gh-pages/docs/next",
  "readme": "../README.md",
  "entryPointStrategy": "expand"
}
