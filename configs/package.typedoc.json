{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "expand",
  "exclude": [
    "**/*spec.ts",
    "**/*spec.tsx",
    "**/*test.ts",
    "**/*test.tsx",
    "**/src/**/test/**",
    "**/src/**/tests/**",
    "**/src/**/typings/**",
  ],
  "hideGenerator": true,
  "searchInComments": true,
  "skipErrorChecking": true,
  "commentStyle": "all",
  "logLevel": "Error"
}
