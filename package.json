{
  "private": true,
  "name": "parent",
  "version": "0.0.0",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "0.0.31",
    "@types/mocha": "^2.2.41",
    "@types/temp": "^0.8.29",
    "@types/webdriverio": "^4.7.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.0",
    "electron-mocha": "^3.5.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "lerna": "^2.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "selenium-standalone": "^6.2.0",
    "temp": "^0.8.3",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.8",
    "typescript": "^2.5.2",
    "uuid": "^3.1.0",
    "wdio-mocha-framework": "^0.5.9",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.6.2"
  },
  "scripts": {
    "prepare": "yarn run rebuild:clean && yarn run build:clean",
    "build": "node scripts/lerna run build --stream",
    "build:clean": "node scripts/lerna run prepare --stream",
    "docs": "node scripts/lerna run docs --scope \"@theia/!(example-)*\" --stream",
    "test": "yarn run test:theia && yarn run test:electron && yarn run test:browser",
    "test:theia": "node scripts/lerna run test --scope \"@theia/!(example-)*\" --parallel",
    "test:browser": "yarn run rebuild:browser && node scripts/lerna run test --scope \"@theia/example-browser\"",
    "test:electron": "yarn run rebuild:electron && node scripts/lerna run test --scope \"@theia/example-electron\"",
    "rebuild:clean": "rimraf .browser_modules",
    "rebuild:browser": "node dev-packages/cli/lib/theia rebuild --target=browser",
    "rebuild:electron": "node dev-packages/cli/lib/theia rebuild --target=electron",
    "rebuild:electron:debug": "DEBUG=electron-rebuild && yarn run rebuild:electron",
    "watch": "lerna run watch --scope \"@theia/!(example-)*\" --parallel",
    "publish": "yarn && yarn test && yarn run publish:latest",
    "publish:latest": "lerna publish --registry=https://registry.npmjs.org/ --exact --skip-git",
    "publish:next": "lerna publish --registry=https://registry.npmjs.org/ --exact --canary=next --npm-tag=next --force-publish --skip-git --yes"
  },
  "workspaces": [
    "dev-packages/*",
    "packages/*",
    "examples/*"
  ]
}