{
  "private": true,
  "name": "browser-example",
  "dependencies": {
    "monaco-css": "^1.3.1",
    "monaco-editor": "^0.8.3",
    "monaco-editor-core": "^0.8.2",
    "monaco-html": "^1.2.1",
    "monaco-json": "^1.2.2",
    "monaco-languages": "^0.7.0",
    "monaco-typescript": "^2.1.2",
    "reflect-metadata": "^0.1.10",
    "theia": "file:../../../Theia"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint -c ../../config/tslint/tslint.json --project ./tsconfig.json",
    "build": "npm run build:app && npm run build:browser",
    "build:app": "tsc && npm run lint",
    "build:browser": "webpack && cp src/client/index.html lib/client",
    "start:app": "node ./lib/app/app.js",
    "start:browser": "concurrently \"npm run watch:dependencies\" \"npm run start:app\" \"webpack-dev-server --open\"",
    "cold:start:browser": "cd ../../ && npm run clean && npm run build && cd examples/browser && npm run clean && npm run build && npm run start:browser",
    "watch:app": "tsc --watch",
    "watch:browser": "npm run build:browser && webpack --watch",
    "watch:dependencies": "node ../../releng/file-dependency-updater/index.js",
    "test:ui": "wdio wdio.conf.js"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/webdriverio": "^4.7.0",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "rimraf": "^2.6.1",
    "selenium-standalone": "^6.2.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.3.2",
    "url-loader": "^0.5.8",
    "wdio-mocha-framework": "^0.5.9",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.6.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
